{
    "builds": [{

    "src": "FindMyNest/wsgi.py",
    
    "use": "@vercel/python",
    
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    
    }],
    
    "routes":[ 
    {
        "src": "/(.*)",
    
        "dest": "FindMyNest/wsgi.py"
    }
    
    ]
}